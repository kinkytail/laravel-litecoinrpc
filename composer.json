{
    "name": "kinky/laravel-litecoinrpc",
    "description": "Litecoin JSON-RPC Service Provider for Laravel",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "kinky",
            "email": "kinky@mail.ru"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "illuminate/support": "^5.1|^6|^7|^8",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "6.3.*"
    },
   "autoload": {
       "psr-4" : {
           "Weisskpub\\Litecoin\\": "src/"
       },
       "files": [
           "src/helpers.php"
       ]
   },
   "extra": {
       "laravel": {
           "providers": [
               "Kinky\\Litecoin\\Providers\\ServiceProvider"
           ]
       },
       "aliases": {
           "Litecoind": "Kinky\\Litecoin\\Facade"
       }
   }
}
